<?xml version='1.0' encoding='UTF-8'?>
<faces-config
  xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
  version="2.3">

  <application>
    <locale-config>
      <default-locale>de</default-locale>
    </locale-config>

    <resource-bundle>
      <base-name>org.cc.torganizer.text</base-name>
      <var>i18nText</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>gender</base-name>
      <var>i18nGender</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>opponentType</base-name>
      <var>i18nOpponentType</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>playerOrder</base-name>
      <var>i18nPlayerOrder</var>
    </resource-bundle>
  </application>
  
</faces-config>